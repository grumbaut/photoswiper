{
  "name": "photoswiper",
  "version": "2.0.7",
  "description": "A plugin for easy and accessible PhotoSwipe initialization",
  "main": "dist/photoswiper.module.js",
  "scripts": {
    "build:browser": "parcel build src/photoswiper.js --global Photoswiper --out-file photoswiper.browser.js",
    "build:module": "parcel build src/photoswiper.js --target node --out-file photoswiper.module.js",
    "build": "npm run build:browser & npm run build:module",
    "dev": "parcel watch ./src/photoswiper.js --global Photoswiper --out-file photoswiper.browser.js",
    "lint": "eslint src",
    "prebuild": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh0ji/photoswiper.git"
  },
  "keywords": [
    "jquery-plugin",
    "photoswipe",
    "accessibility",
    "a11y"
  ],
  "author": "Evan Yamanishi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sh0ji/photoswiper/issues"
  },
  "homepage": "https://github.com/sh0ji/photoswiper#readme",
  "dependencies": {
    "photoswipe": "^4.1.1",
    "tabtrap": "^1.2.6"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "parcel-bundler": "^1.10.3"
  }
}
