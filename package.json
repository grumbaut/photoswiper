{
  "name": "photoswiper",
  "version": "2.0.7",
  "description": "A plugin for easy and accessible PhotoSwipe initialization",
  "main": "dist/photoswiper.module.js",
  "scripts": {
    "build:browser": "browserify src/photoswiper.js -s photoswiper -t [ babelify --presets [ es2015 ] --plugins [ add-module-exports ] ] | derequire > dist/photoswiper.browser.js",
    "build:module": "babel src/photoswiper.js -o dist/photoswiper.module.js",
    "build": "npm run build:browser & npm run build:module",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh0ji/photoswiper.git"
  },
  "keywords": [
    "jquery-plugin",
    "photoswipe",
    "accessibility",
    "a11y"
  ],
  "author": "Evan Yamanishi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sh0ji/photoswiper/issues"
  },
  "homepage": "https://github.com/sh0ji/photoswiper#readme",
  "dependencies": {
    "photoswipe": "^4.1.1",
    "tabtrap": "^1.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "derequire": "^2.0.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
